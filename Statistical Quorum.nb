Cell[BoxData[
 RowBox[{
  RowBox[{"computeCompetingCredibility", "[", "voteStatistics_", "]"}], ":=", 
  
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Computes", " ", "the", " ", "probability", " ", "that", " ", "the", " ", 
     "first", " ", 
     RowBox[{"item", "'"}], "s", " ", "\"\<yes-bias\>\"", " ", "is", " ", 
     "greater", " ", "than", " ", "that", " ", "of", " ", "all", " ", "other",
      " ", 
     RowBox[{"items", "."}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"credibility", "=", "1"}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"a", ",", "b"}], "}"}], "=", 
     RowBox[{"{", 
      RowBox[{"1", ",", "1"}], "}"}]}], ";", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"1", ",", "1"}], "}"}], " ", "corresponds", " ", "to", " ", 
      "a", " ", "uniform", " ", "prior"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"prior", "[", 
      RowBox[{"p_", ",", "yes_", ",", "no_"}], "]"}], ":=", 
     RowBox[{"PDF", "[", 
      RowBox[{
       RowBox[{"BetaDistribution", "[", 
        RowBox[{
         RowBox[{"yes", "+", "a"}], ",", 
         RowBox[{"no", "+", "b"}]}], "]"}], ",", "p"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "yes1", ",", "no1", ",", "yesVotesNeeded1", ",", "remainingVoters1"}], 
      "}"}], "=", 
     RowBox[{"voteStatistics", "[", 
      RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Do", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"{", 
         RowBox[{
         "yesI", ",", "noI", ",", "yesVotesNeededI", ",", 
          "remainingVotersI"}], "}"}], "=", 
        RowBox[{"voteStatistics", "[", 
         RowBox[{"[", "i", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"diff", "=", 
        RowBox[{"Ceiling", "[", 
         RowBox[{"yes1", "-", "yesI"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"credibility", "*=", 
        RowBox[{"1", "-", 
         RowBox[{"Sum", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{"Binomial", "[", 
             RowBox[{"remainingVoters1", ",", "numYes1"}], "]"}], "*", 
            RowBox[{"Binomial", "[", 
             RowBox[{"remainingVotersI", ",", "numYesI"}], "]"}], "*", 
            "\[IndentingNewLine]", 
            RowBox[{"NIntegrate", "[", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{
               RowBox[{"p1", "^", 
                RowBox[{"(", "numYes1", ")"}]}], "*", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"1", "-", "p1"}], ")"}], "^", 
                RowBox[{"(", 
                 RowBox[{"remainingVoters1", "-", "numYes1"}], ")"}]}], "*", 
               "\[IndentingNewLine]", 
               RowBox[{"pI", "^", 
                RowBox[{"(", "numYesI", ")"}]}], "*", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"1", "-", "pI"}], ")"}], "^", 
                RowBox[{"(", 
                 RowBox[{"remainingVotersI", "-", "numYesI"}], ")"}]}], "*", 
               "\[IndentingNewLine]", 
               RowBox[{"prior", "[", 
                RowBox[{"p1", ",", "yes1", ",", "no1"}], "]"}], "*", 
               RowBox[{"prior", "[", 
                RowBox[{"pI", ",", "yesI", ",", "noI"}], "]"}]}], 
              "\[IndentingNewLine]", ",", 
              RowBox[{"{", 
               RowBox[{"p1", ",", "0", ",", "1"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"pI", ",", "0", ",", "1"}], "}"}]}], "]"}]}], 
           "\[IndentingNewLine]", ",", 
           RowBox[{"{", 
            RowBox[{"numYes1", ",", "0", ",", "remainingVoters1"}], "}"}], 
           ",", 
           RowBox[{"{", 
            RowBox[{"numYesI", ",", 
             RowBox[{"diff", "+", "numYes1"}], ",", "remainingVotersI"}], 
            "}"}]}], "]"}]}]}], ";"}], "\[IndentingNewLine]", ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "2", ",", 
        RowBox[{"Length", "[", "voteStatistics", "]"}]}], "}"}]}], "]"}], ";",
     "\[IndentingNewLine]", "credibility"}], "\[IndentingNewLine]", 
   ")"}]}]], "Input",
